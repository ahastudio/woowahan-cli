#!/usr/bin/env node
'use strict';

var program = require('commander');
var manifest = require('../package.json');

program
	.version(manifest.version)
	.usage('command <options>')
	.command('new', 'woowahan-cli new AppName')
	.command('init', 'woowahan-cli init')
	.command('view', 'woowahan-cli view viewName < -c , --collection>')
	.command('reducer', 'woowahan-cli reducer reducerName <ActionName>')
	.parse(process.argv);
