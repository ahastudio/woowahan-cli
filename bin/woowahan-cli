#!/usr/bin/env node
'use strict';

var program = require('commander');
var manifest = require('../package.json');

program
	.version(manifest.version)
	.usage('command <options>')
	.command('new', 'Generate Woowahan App')
	.command('init', 'Generate Woowahan App in current directory.')
	.command('view', 'Generate Woowahan views.')
	.command('reducer', 'Generate Woowahan reducer & action.')
	.parse(process.argv);
